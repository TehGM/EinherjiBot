{
  "Commands": {
    "Prefix": "/.",
    "AcceptMentionPrefix": false
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "retainedFileCountLimit": 2,
                "fileSizeLimitBytes": 1048576
              }
            }
          ]
        }
      }
    ]
  }
}
